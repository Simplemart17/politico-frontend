{
  "name": "politico-frontend",
  "version": "1.0.0",
  "description": "This Frontend built with React/Redux consumes the Politico App API",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "coverage": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "dev": "webpack serve --mode development",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "heroku-postbuild": "rimraf dist && npm run build",
    "start": "node server.js",
    "test": "jest",
    "test:coveralls": "jest -u | coveralls",
    "test:dev": "jest --watch -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-fix",
      "git add --force"
    ],
    "*.jsx": [
      "npm run lint-fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simplemart17/politico-frontend.git"
  },
  "keywords": [
    "politico",
    "news",
    "react/redux"
  ],
  "author": "Martins Aloba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Simplemart17/politico-frontend/issues"
  },
  "homepage": "https://github.com/Simplemart17/politico-frontend#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "coveralls": "^3.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.0.0-alpha.14",
    "husky": "^2.7.0",
    "jest": "^26.6.3",
    "lint-staged": "^8.2.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "style-loader": "^0.23.1",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "babel-preset-react-app": "^8.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "express": "^4.17.3",
    "jest-localstorage-mock": "^2.4.3",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-content-loader": "^4.3.4",
    "react-dom": "^16.14.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.2",
    "react-redux-hook": "0.0.1",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.9.0",
    "react-testing-library": "^7.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.3.1",
    "styled-components": "^4.4.1",
    "sweetalert": "^2.1.2"
  }
}
